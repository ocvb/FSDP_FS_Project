{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@src/*": [
                "src/*"
            ],
            "@validations/*": [
                "src/Middlewares/Validations/*"
            ],
            "@models/*": [
                "src/Models/*"
            ],
            "@api/*": [
                "src/APIs/*"
            ],
            "@schema/*": [
                "src/Schemas/*"
            ],
            "@middleware/*": [
                "src/Middlewares/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}